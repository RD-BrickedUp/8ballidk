import random
random_number = random.randint(1,9)
string_list = ["Yes - definitely.", "It is decidedly so.", "Without a doubt.", "Reply hazy, try again.", "Ask again later.", "Better not tell you now.", "My sources say no.", "Outlook not so good.", "Very doubtful."]
name = input("What is your name? ")
question = input("Input Your Question: ")
answer = (random.choice(string_list))

print(name + " asks: " + question)
print("Magic 8-Ball's answer: " + answer)
print("Magic 8-Ball's answer: " + answer)
